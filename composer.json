{
    "name": "stormiix/laravel-mix-twig-extension",
    "license": "MIT",
    "type": "library",
    "description" : "Laravel Mix twig extension",
    "keywords": ["php", "laravel", "twig", "extension", "mix"],
    "autoload": {
        "psr-4": {
            "Stormiix\\Twig\\Extension\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Stormiix\\Twig\\Extension\\Tests\\": "tests/"
        }
    },
    "require": {
        "php": ">=5.5.9",
        "twig/twig": "~1.28|~2.0"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
